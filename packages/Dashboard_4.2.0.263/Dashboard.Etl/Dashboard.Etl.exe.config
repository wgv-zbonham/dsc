<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="UnicastBusConfig" type="NServiceBus.Config.UnicastBusConfig, NServiceBus.Core" />
    <section name="MessageForwardingInCaseOfFaultConfig" type="NServiceBus.Config.MessageForwardingInCaseOfFaultConfig, NServiceBus.Core" />
  </configSections>
  <appSettings>
    <add key="CommandTimeout" value="600" />
    <add key="BatchSize" value="10" />
    <add key="ClientSettingsProvider.ServiceUri" value="" />
  </appSettings>
  <connectionStrings>
    <add name="SourceDb" connectionString="Server=.;Database=WGEvidenceLibrary;Trusted_Connection=True;" />
    <add name="DestinationDb" connectionString="Server=.;Database=WGStaging;Trusted_Connection=True;" />
    <add name="LoggingDb" connectionString="Server=.;Database=WGReport;Trusted_Connection=True;" />
  </connectionStrings>
  <system.diagnostics>
    <sources>
      <source name="WatchGuard" switchName="sourceSwitch" switchType="System.Diagnostics.SourceSwitch">
        <listeners>
          <clear />
          <add name="WatchGuard" />
          <add name="EventLog" />
        </listeners>
      </source>
    </sources>
    <sharedListeners>
      <add name="WatchGuard" type="WatchGuard.Dashboard.Core.Logging.WatchGuardTraceListener, WatchGuard.Dashboard.Core" initializeData="LoggingDb" />
      <add name="EventLog" type="System.Diagnostics.EventLogTraceListener" initializeData="Application" />
    </sharedListeners>
    <switches>
      <!-- this knob controls the logging level -->
      <!-- Error, Warning, Informational, Debug (Verbose) -->
      <add name="sourceSwitch" value="Verbose" />
    </switches>
  </system.diagnostics>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
  </startup>
  <MessageForwardingInCaseOfFaultConfig ErrorQueue="Etl-Error" />
  <UnicastBusConfig>
    <MessageEndpointMappings>
      <add Assembly="Dashboard.Etl.Types" Type="Dashboard.Etl.Types.Officer.AddOfficerCommand" Endpoint="Etl-Dst-OfficerDimension" />
    </MessageEndpointMappings>
  </UnicastBusConfig>
  <system.web>
    <membership defaultProvider="ClientAuthenticationMembershipProvider">
      <providers>
        <add name="ClientAuthenticationMembershipProvider" type="System.Web.ClientServices.Providers.ClientFormsAuthenticationMembershipProvider, System.Web.Extensions, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" serviceUri="" />
      </providers>
    </membership>
    <roleManager defaultProvider="ClientRoleProvider" enabled="true">
      <providers>
        <add name="ClientRoleProvider" type="System.Web.ClientServices.Providers.ClientRoleProvider, System.Web.Extensions, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" serviceUri="" cacheTimeout="86400" />
      </providers>
    </roleManager>
  </system.web>
</configuration>